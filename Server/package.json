{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node index.js",
    "dev": "cross-env NODE_ENV=development nodemon index.js",
    "build": "npm run build:ui",
    "build:ui": "@powershell if (Test-Path dist) { Remove-Item -Recurse -Force dist } && cd ../Frontend && npm run build && @powershell Copy-Item dist -Recurse ../Backend",
    "deploy:full": "npm run build:ui && git add . && git commit -m uibuild && git push",
    "lint": "eslint .",
    "test": "cross-env NODE_ENV=test node --test"
  },
  "author": "Krishnendu M R-SAAS TECHNICAL DESIGN",
  "license": "ISC",
  "description": "This is the backend of the official Techofes 2025 website",
  "dependencies": {
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "cloudinary": "^2.5.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.10.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@stylistic/eslint-plugin-js": "^2.8.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.11.1",
    "eslint-plugin-react": "^7.37.0",
    "globals": "^15.9.0",
    "nodemon": "^3.1.4",
    "supertest": "^7.0.0"
  }
}
